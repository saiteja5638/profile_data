<mvc:View xmlns:core="sap.ui.core" controllerName="excelfileupload.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"  xmlns:u="sap.ui.unified" displayBlock="true"
    xmlns="sap.m">
    <Page id="page"  class="oPage" title="{i18n>title}">

	<customHeader>
	<Toolbar id="_IDGenToolbar1" 
	>
	
	 <Text id="_IDGenText1"    text="Student Mark Data"  class="Title_text" />
	</Toolbar>
	</customHeader>
	        
        		<subHeader>
						<Bar id="_IDGenBar1">
							<contentLeft>
								<SearchField id="_IDGenSearchField1" liveChange="OnSearch1" width="100%"/>
							</contentLeft>
							<contentMiddle>
								<HBox id="_IDGenHBox1" class="Input1">
									<!-- <ComboBox class="sapUiContentPadding" id="mySelect" placeholder="Search by col name" selectionChange="onSelectionChange">
		                                <items>
										<core:Item id="_IDGenItem1" key="ID" text="ID" ></core:Item>
										<core:Item id="_IDGenItem2" key="NAME" text="NAME" ></core:Item>
										</items>
									</ComboBox> -->
									<MultiComboBox id="_IDGenMultiComboBox1"   placeholder="Choose required data"  selectionFinish=".onSelectionFinish"  items="{/items}" showSecondaryValues= "true" >
						              <items>
									  <core:ListItem id="_IDGenListItem1"  key="{ID}"  text="{ID}"  additionalText="{NAME}" ></core:ListItem>
									  
									  </items>
									</MultiComboBox>
								</HBox>
							</contentMiddle>
							<contentRight>
									  <DateRangeSelection id="dateRangeSelection" change="onDateRangeSelectionChange" />
							</contentRight>
							<contentRight>
								<Button id="_IDGenButton2" text="Next View" icon="sap-icon://add" press="navtoBAck"/>
							</contentRight>
							<contentRight>
								<Button id="_IDGenButton3" text="Download File" icon="sap-icon://download" press="oncreateFilename"/>
							</contentRight>
							<contentRight>
								<u:FileUploader id="FileUploaderId" sameFilenameAllowed="true" iconOnly="false" buttonOnly="true" fileType="XLSX,xlsx"
									icon="sap-icon://upload" iconFirst="true" style="Emphasized" change="onUpload"/>
							</contentRight>
						</Bar>
					</subHeader>
					<Table id="table"  	alternateRowColors="true"></Table>	
    </Page>
</mvc:View>
