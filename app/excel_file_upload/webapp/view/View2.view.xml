<mvc:View xmlns:tnt="sap.tnt" xmlns:m="sap.m" controllerName="excelfileupload.controller.View2"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true" 	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:l="sap.ui.layout" xmlns:viz="sap.viz.ui5.controls"
	xmlns:viz.data="sap.viz.ui5.data" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:suite="sap.suite.ui.microchart"
	xmlns:w="sap.ui.integration.widgets" xmlns:cssgrid="sap.ui.layout.cssgrid" height="100%" >
	

	<m:Page id="_IDGenPage1"  title="" >

	<m:customHeader>
	<m:Toolbar id="_IDGenToolbar1" >
	<m:Button id="_IDGenButton1"  type="Back"   press="navtoBAck" ></m:Button>
	<m:Text id="_IDGenText1" text="Students Score_Report" ></m:Text>
	</m:Toolbar>
	</m:customHeader>
	
	
					<viz:VizFrame id="Chart1" width="100%" height="50%" vizType="column" class="StackedBarClass"
						vizProperties="{title: {text : 'Score Analysis'}, plotArea: { drawingEffect: 'glossy'}}"     uiConfig="{applicationSet:'fiori'}">
						<viz:dataset>
							<viz.data:FlattenedDataset id="_IDGenFlattenedDataset1" data="{/items}">
								<viz.data:dimensions>
									<viz.data:DimensionDefinition id="_IDGenDimensionDefinition1" name="Date" value="{Date}"/>
								</viz.data:dimensions>
								<viz.data:measures>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition1" name="SBP CORP" value="{SBP CORP}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition2" name="MURALI" value="{MURALI}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition3" name="SUBHAM" value="{SUBHAM}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition5" name="MOKSHA" value="{MOKSHA}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition8" name="MAHESH" value="{MAHESH}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition10" name="TEJA SWI" value="{TEJA SWI}"/>
										<viz.data:MeasureDefinition id="_IDGenMeasureDefinition85" name="RAMYA" value="{RAMYA}"/>
									
										<viz.data:MeasureDefinition id="_IDGenMeasureDefinition855" name="SAI TEJA" value="{SAI TEJA}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition10FD" name="SUPRIYA" value="{SUPRIYA}"/>
								
								</viz.data:measures>
							</viz.data:FlattenedDataset>
						</viz:dataset>
						<viz:feeds>
							
							<viz.feeds:FeedItem id="_IDGenFeedItem3" uid="valueAxis" type="Measure" values="SBP CORP,SUBHAM,MURALI,MOKSHA,MAHESH,TEJA SWI,RAMYA,SAI TEJA,SUPRIYA"/>
							<viz.feeds:FeedItem id="_IDGenFeedItem4" uid="categoryAxis" type="Dimension" values="Date"/>
					
						</viz:feeds>
					</viz:VizFrame>

						<viz:VizFrame id="Chart2" width="100%" height="50%" vizType="column" class="StackedBarClass"
						vizProperties="{title: {text : 'Over All Score'}, plotArea: { drawingEffect: 'glossy'}}"     uiConfig="{applicationSet:'fiori'}">
						<viz:dataset>
							<viz.data:FlattenedDataset id="_IDGenFlattenedDataset2" data="{/items}">
								<viz.data:dimensions>
									<viz.data:DimensionDefinition id="_IDGenDimensionDefinition2" name="Date" value="Average of all"/>
								</viz.data:dimensions>
								<viz.data:measures>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition1f" name="SBP CORP" value="{SBP CORP}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition2sd" name="MURALI" value="{MURALI}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition3sd" name="SUBHAM" value="{SUBHAM}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition5s" name="MOKSHA" value="{MOKSHA}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition8s" name="MAHESH" value="{MAHESH}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinition10s" name="TEJA SWI" value="{TEJA SWI}"/>
										<viz.data:MeasureDefinition id="_IDGenMeasureDefinitiosn85" name="RAMYA" value="{RAMYA}"/>
										<viz.data:MeasureDefinition id="_IDGenMeasureDefinsition855" name="SAI TEJA" value="{SAI TEJA}"/>
									<viz.data:MeasureDefinition id="_IDGenMeasureDefinitions10FD" name="SUPRIYA" value="{SUPRIYA}"/>
								
								</viz.data:measures>
							</viz.data:FlattenedDataset>
						</viz:dataset>
						<viz:feeds>
							
							<viz.feeds:FeedItem id="_IDGenFeesdItem3" uid="valueAxis" type="Measure" values="SBP CORP,SUBHAM,MURALI,MOKSHA,MAHESH,TEJA SWI,RAMYA,SAI TEJA,SUPRIYA"/>
							<viz.feeds:FeedItem id="_IDGenFesedItem4" uid="categoryAxis" type="Dimension" values="Date"/>
					
						</viz:feeds>
					</viz:VizFrame>
	
	</m:Page>

</mvc:View>