<mvc:View controllerName="flexiblecolumnlayout.controller.View2"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" showHeader="false">
                    <Tree
            id="Tree"
            items="{path: '/'}" mode="MultiSelect"  selectionChange="dataCall" >
            			<headerToolbar>
				<OverflowToolbar id="_IDGenOverflowToolbar1">
					<Title
						id="TreeId"  />
					<ToolbarSpacer id="_IDGenToolbarSpacer1" />
                    <Button id="_IDGenButton1"  icon="sap-icon://decline" press="handleClose" ></Button>
				</OverflowToolbar>
			</headerToolbar>
            <StandardTreeItem id="_IDGenStandardTreeItem1" title="{text}"  selected="{selected}"   />
           </Tree>
    </Page>
</mvc:View>