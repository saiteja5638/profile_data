<mvc:View xmlns:odata="sap.ui.comp.odata" xmlns:core="sap.ui.core" xmlns:cards="sap.f.cards" xmlns:widgets="sap.ui.integration.widgets" xmlns:f="sap.f" controllerName="netflix.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}"  class="main_view"  showHeader="false"  >   
    
     <f:Card id="_IDGenCard1"  class="Card1"  >
  <f:content>
    <Toolbar id="_IDGenToolbar1"   class="Nav_bar" >
      <Text id="_IDGenText1"  class="LOGO_netflix"  text="NETFLIX"   ></Text>
      <ToolbarSpacer id="_IDGenToolbarSpacer1" ></ToolbarSpacer> 
      <ToolbarSpacer id="_IDGenToolbarSpacer2" ></ToolbarSpacer>
      <ComboBox id="_IDGenComboBox1"  selectedKey="English" class="Selection_box"   >
      <items>
      <core:Item id="_IDGenItem1" text="English" key="English"  ></core:Item>
      <core:Item id="_IDGenItem2" text="Hindi"  key="Hindi"  ></core:Item>
      </items>
      </ComboBox>
      <Button id="_IDGenButton2" text="Signin" press="onContactPress"/>
    </Toolbar>
  </f:content>

     </f:Card>

    </Page>
</mvc:View>
