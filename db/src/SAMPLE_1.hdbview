VIEW "SAMPLE_1"
AS SELECT   CATALOGSERVICE_INTERACTIONS_STUDENT_DATA.NAME ,CATALOGSERVICE_INTERACTIONS_STUDENT_MARKS.DATE,CATALOGSERVICE_INTERACTIONS_STUDENT_MARKS.MARKS

FROM CATALOGSERVICE_INTERACTIONS_STUDENT_DATA

INNER JOIN CATALOGSERVICE_INTERACTIONS_STUDENT_MARKS ON CATALOGSERVICE_INTERACTIONS_STUDENT_DATA.ID=CATALOGSERVICE_INTERACTIONS_STUDENT_MARKS.ID

ORDER BY DATE ASC