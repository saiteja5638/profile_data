VIEW "STUD_PROGRESS1"

AS
SELECT "CATALOGSERVICE_INTERACTIONS_STUDENT_DATA".ID ,"CATALOGSERVICE_INTERACTIONS_STUDENT_DATA".NAME,"CATALOGSERVICE_INTERACTIONS_STUDENT_DATA".DATEOFBIRTH,"CATALOGSERVICE_INTERACTIONS_STUDENT_DATA".CONTACT , "CATALOGSERVICE_INTERACTIONS_STUDENT_MARKS".MARKS,"CATALOGSERVICE_INTERACTIONS_STUDENT_MARKS".DATE

FROM "CATALOGSERVICE_INTERACTIONS_STUDENT_DATA" INNER JOIN "CATALOGSERVICE_INTERACTIONS_STUDENT_MARKS" ON "CATALOGSERVICE_INTERACTIONS_STUDENT_DATA".ID ="CATALOGSERVICE_INTERACTIONS_STUDENT_MARKS".ID

