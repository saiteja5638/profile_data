<mvc:View controllerName="flexiblecolumnlayout.controller.View2"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="Config Options">
                    <Tree
            id="Tree"
            items="{path: '/'}" mode="MultiSelect"  selectionChange="dataCall" >
            			<headerToolbar>
				<OverflowToolbar id="_IDGenOverflowToolbar1">
					<Title
						id="TreeId"  />
					<ToolbarSpacer id="_IDGenToolbarSpacer1" />
				</OverflowToolbar>
			</headerToolbar>
            <StandardTreeItem id="_IDGenStandardTreeItem1" title="{text}"  selected="false" />
           </Tree>
    </Page>
</mvc:View>